{
    "apps": [
        {
            "command": "/home/user/projects/rfmeask/rfmeas --config /home/user/settings.json --runtime /home/user/runtime.json > /home/user/rfmeas_log",
            "name": "rfmeas",
            "order": 1,
            "restart": "always"
        },
        {
            "command": "/usr/local/bin/crowker --udp 10009",
            "name": "crowker",
            "order": 2,
            "restart": "always"
        },
        {
            "command": "/usr/local/bin/ctrans --udp 10014 --serial_v1 /dev/ttyS3 --noconsole",
            "name": "ctrans",
            "order": 3,
            "restart": "always"
        },
        {
            "command": "/home/user/projects/rfmeask_dataproxy/dataproxy",
            "name": "dataproxy",
            "order": 4,
            "restart": "always"
        }
    ]
}
